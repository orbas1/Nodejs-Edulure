{
  "name": "edulure-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.12.2",
    "npm": ">=10.5.0"
  },
  "scripts": {
    "dev": "node ./scripts/run-vite.mjs dev",
    "build": "node ./scripts/run-vite.mjs build",
    "preview": "node ./scripts/run-vite.mjs preview",
    "lint": "eslint \"src\"",
    "lint:fix": "eslint \"src\" --fix",
    "test": "vitest run --coverage",
    "test:release": "vitest run test/release --reporter=basic",
    "audit:dependencies": "npm audit --audit-level=moderate",
    "sdk:build": "npm run --prefix ../sdk-typescript build",
    "predev": "npm run sdk:build",
    "prebuild": "npm run sdk:build"
  },
  "dependencies": {
    "@edulure/api-sdk": "file:../sdk-typescript",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.1.3",
    "axios": "^1.7.7",
    "clsx": "^2.1.0",
    "d3-color": "^3.1.0",
    "d3-geo": "^3.1.1",
    "epubjs": "^0.3.93",
    "idb-keyval": "^6.2.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "recharts": "^3.2.1",
    "socket.io-client": "^4.8.1",
    "topojson-client": "^3.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "jest-axe": "^9.0.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "globals": "^15.8.0",
    "jsdom": "^27.0.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.1",
    "vite": "^7.1.3",
    "vitest": "^3.2.4"
  }
}
