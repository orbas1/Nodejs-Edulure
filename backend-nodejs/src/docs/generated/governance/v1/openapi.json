                  "ownerEmail": {
                    "type": "string"
                  },
                  "riskTier": {
                    "type": "string"
                  },
                  "renewalDate": {
                    "type": "string",
                    "format": "date",
                    "nullable": true
                  },
                  "terminationNoticeDate": {
                    "type": "string",
                    "format": "date",
                    "nullable": true
                  },
                  "obligations": {
                    "type": "array",
