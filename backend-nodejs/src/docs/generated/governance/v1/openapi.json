  "paths": {
    "/governance/communications": {
      "get": {
        "summary": "List governance communications",
        "tags": [
          "Governance"
        ],
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "parameters": [
          {
            "name": "status",
            "in": "query",
            "description": "Filter by communication status (comma separated or repeated).",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "audience",
            "in": "query",
            "description": "Filter by communication audience (comma separated or repeated).",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "channel",
            "in": "query",
            "description": "Filter by communication channel (comma separated or repeated).",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "scheduledBefore",
            "in": "query",
            "description": "Return communications scheduled before the provided date-time (ISO 8601).",
            "schema": {
              "type": "string",
              "format": "date-time"
            }
          },
          {
            "name": "ownerEmail",
            "in": "query",
            "description": "Filter by owner email.",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "limit",
            "in": "query",
            "schema": {
              "type": "integer",
              "minimum": 1,
              "maximum": 100,
              "default": 25
            },
            "description": "Maximum number of communications to return (1-100)."
          },
          {
            "name": "offset",
            "in": "query",
            "schema": {
              "type": "integer",
              "minimum": 0,
              "default": 0
            },
            "description": "Offset into the communication result set."
          }
        ],
        "responses": {
          "200": {
            "description": "Communications retrieved.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GovernanceCommunicationListResponse"
                }
              }
            }
          },
          "401": {
            "description": "Missing or invalid token"
          },
          "403": {
            "description": "Governance APIs disabled for this role"
          }
        }
      },
      "post": {
        "summary": "Schedule governance communication",
        "tags": [
          "Governance"
        ],
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "audience": {
                    "type": "string"
                  },
                  "channel": {
                    "type": "string"
                  },
                  "subject": {
                    "type": "string"
                  },
                  "body": {
                    "type": "string"
                  },
                  "scheduleAt": {
                    "type": "string",
                    "format": "date-time",
                    "nullable": true
                  },
                  "status": {
                    "type": "string"
                  }
                },
                "required": [
                  "audience",
                  "channel",
                  "subject"
                ],
                "additionalProperties": true
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Communication scheduled.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GovernanceCommunication"
                }
              }
            }
          },
          "400": {
            "description": "Invalid payload"
          },
          "401": {
            "description": "Missing or invalid token"
          },
          "403": {
            "description": "Governance APIs disabled for this role"
          }
        }
      }
    },
    "/governance/communications/{communicationId}/metrics": {
      "post": {
        "summary": "Record communication metrics",
        "tags": [
          "Governance"
        ],
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "parameters": [
          {
            "name": "communicationId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "Communication identifier."
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "delivered": {
                    "type": "integer"
                  },
                  "engagementRate": {
                    "type": "number"
                  },
                  "status": {
                    "type": "string"
                  },
                  "sentAt": {
                    "type": "string",
                    "format": "date-time"
                  }
                },
                "additionalProperties": true
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Communication metrics recorded.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GovernanceCommunication"
                }
              }
            }
          },
          "401": {
            "description": "Missing or invalid token"
          },
          "403": {
            "description": "Governance APIs disabled for this role"
          },
          "404": {
            "description": "Communication not found"
          }
        }
      }
    },
