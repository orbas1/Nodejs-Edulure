                      "type": "object",
                      "properties": {
                        "id": {
                          "type": "string"
                        },
                        "description": {
                          "type": "string"
                        },
                        "owner": {
                          "type": "string"
                        },
                        "dueAt": {
                          "type": "string",
                          "format": "date-time",
                          "nullable": true
                        },
                        "completedAt": {
                          "type": "string",
                          "format": "date-time",
                          "nullable": true
                        },
                        "status": {
                          "type": "string"
                        }
                      },
                      "required": [
                        "id",
                        "description",
                        "owner",
                        "status"
                      ],
                      "additionalProperties": false
                    }
                  }
                },
                "additionalProperties": true
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Contract updated.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GovernanceContract"
                }
              }
            }
          },
          "401": {
            "description": "Missing or invalid token"
          },
          "403": {
            "description": "Governance APIs disabled for this role"
          },
          "404": {
            "description": "Contract not found"
          }
        }
