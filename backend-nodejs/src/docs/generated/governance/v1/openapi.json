    "/governance/review-cycles": {
      "get": {
        "summary": "List governance review cycles",
        "tags": [
          "Governance"
        ],
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "parameters": [
          {
            "name": "status",
            "in": "query",
            "description": "Filter by review cycle status (comma separated or repeated).",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "cycleName",
            "in": "query",
            "description": "Filter by review cycle name search term.",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "startAfter",
            "in": "query",
            "description": "Return cycles starting on or after the provided date (YYYY-MM-DD).",
            "schema": {
              "type": "string",
              "format": "date"
            }
          },
          {
            "name": "endBefore",
            "in": "query",
            "description": "Return cycles ending on or before the provided date (YYYY-MM-DD).",
            "schema": {
              "type": "string",
              "format": "date"
            }
          },
          {
            "name": "onlyUpcoming",
            "in": "query",
            "description": "Only include upcoming or in-progress cycles when true.",
            "schema": {
              "type": "boolean"
            }
          },
          {
            "name": "overdue",
            "in": "query",
            "description": "Only include cycles with overdue milestones when true.",
            "schema": {
              "type": "boolean"
            }
          },
          {
            "name": "limit",
            "in": "query",
            "schema": {
              "type": "integer",
              "minimum": 1,
              "maximum": 100,
              "default": 25
            },
            "description": "Maximum number of review cycles to return (1-100)."
          },
          {
            "name": "offset",
            "in": "query",
            "schema": {
              "type": "integer",
              "minimum": 0,
              "default": 0
            },
            "description": "Offset into the review cycle result set."
          }
        ],
        "responses": {
          "200": {
            "description": "Review cycles retrieved.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GovernanceReviewCycleListResponse"
                }
              }
            }
          },
          "401": {
            "description": "Missing or invalid token"
          },
          "403": {
            "description": "Governance APIs disabled for this role"
          }
        }
      }
    },
